generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id         String  @id @default(uuid()) // Matches Supabase Auth UID
  email      String  @unique
  name       String?
  avatar_url String?
  credits    Int      @default(20)
  createdAt  DateTime @default(now())
}
